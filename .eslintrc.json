{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    // "comma-dangle": ["error", "never"],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "parser": "flow",
        "trailingComma": "es6"
      }
    ]
  }
}
